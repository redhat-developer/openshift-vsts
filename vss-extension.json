{
    "manifestVersion": 1,
    "id": "openshift-vsts",
    "publisher": "HardyFerentschik",
    "version": "0.0.46",
    "name": "OpenShift 3 Integration",
    "description": "OpenShift tasks for Azure DevOps",
    "public": false,
    "categories": [
        "Build and release"
    ],
    "tags": [
        "utility",
        "tasks",
        "openshift"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/hferentschik/openshift-vsts/blob/master/docs/getting-started.md"
        },
        "issues": {
            "uri": "https://github.com/hferentschik/openshift-vsts/issues"
        },
        "repository": {
            "uri": "https://github.com/hferentschik/openshift-vsts"
        }
	},
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "openshift",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "OpenShift",
                "uri": "openshift-vsts.html"
            }
        },
        {
            "id": "oc-cmd-task",
            "description": "Task to run oc command.",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": {
              "name": "tasks/oc-cmd"
            }
        }              
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "openshift-vsts.html",
            "addressable": true
        },
        {
            "path": "tasks/oc-cmd"
        }
    ]
}
